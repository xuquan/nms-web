<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.otn.collector.huawei.delivery.beans.equipment.Cabinet" table="OTN_DELIVERY_CABINET">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="increment" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME" />
        </property>
        <property name="equipmentRoomName" type="java.lang.String">
            <column name="EQUIPMENTROOMNAME" />
        </property>
        <property name="containedShelfList" type="java.lang.String">
            <column name="CONTAINEDSHELFLIST" />
        </property>
        <property name="type" type="java.lang.String">
            <column name="TYPE" />
        </property>
        <property name="height" type="short">
            <column name="HEIGHT" />
        </property>
        <property name="width" type="short">
            <column name="WIDTH" />
        </property>
        <property name="depth" type="short">
            <column name="DEPTH" />
        </property>
        <property name="voltage" type="int">
            <column name="VOLTAGE" />
        </property>
        <property name="powerBoxType" type="java.lang.String">
            <column name="POWERBOXTYPE" />
        </property>
        <property name="memo" type="java.lang.String">
            <column name="MEMO" />
        </property>
        <property name="physicalLocationInfoId" type="int">
        	<column name="physicalLocationInfoId"/>
        </property>
    </class>
</hibernate-mapping>
