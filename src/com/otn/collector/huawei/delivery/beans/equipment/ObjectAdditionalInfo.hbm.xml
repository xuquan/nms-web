<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.otn.collector.huawei.delivery.beans.equipment.ObjectAdditionalInfo" table="OTN_DELIVERY_OBJECTADDITIONALINFO">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="increment" />
        </id>
        <property name="meOrHolderId" type="java.lang.String">
            <column name="MEORHOLDERID" />
        </property>
        <property name="objectNameStr" type="java.lang.String">
            <column name="OBJECTNAMESTR" />
        </property>
        <property name="additionalInfoStr" type="java.lang.String">
            <column name="ADDITIONALINFOSTR" length="1024"/>
        </property>
    </class>
</hibernate-mapping>
