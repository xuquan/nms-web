<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.otn.collector.huawei.delivery.beans.equipment.EquipmentAdditionalInfo" table="OTN_DELIVERY_EQUIPMENTADDITIONALINFO">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="increment" />
        </id>
        <property name="alarmSeverity" type="java.lang.String">
            <column name="ALARMSEVERITY" />
        </property>
        <property name="hardwareVersion" type="java.lang.String">
            <column name="HARDWAREVERSION" />
        </property>
        <property name="port_XX_SFP" type="java.lang.String">
            <column name="PORT_XX_SFP" />
        </property>
        <property name="port_X_SFP_BarCode" type="java.lang.String">
            <column name="PORT_X_SFP_BARCODE" />
        </property>
        <property name="manufactured" type="java.lang.String">
            <column name="MANUFACTURED" />
        </property>
        <property name="clei" type="java.lang.String">
            <column name="CLEI" />
        </property>
        <property name="port_X_SFP_CLEI" type="java.lang.String">
            <column name="PORT_X_SFP_CLEI" />
        </property>
        <property name="port_X_SFP_Item" type="java.lang.String">
            <column name="PORT_X_SFP_ITEM" />
        </property>
        <property name="port_X_SFP_Manufactured" type="java.lang.String">
            <column name="PORT_X_SFP_MANUFACTURED" />
        </property>
        <property name="equipmentNameStr" type="java.lang.String">
            <column name="equipmentNameStr" />
        </property>
    </class>
</hibernate-mapping>
