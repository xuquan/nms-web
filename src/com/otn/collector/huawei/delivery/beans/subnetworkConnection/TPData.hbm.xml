<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.otn.collector.huawei.delivery.beans.subnetworkConnection.TPData" table="OTN_DELIVERY_TPDATA">
        <id name="id" type="int">
            <column name="ID"/>
            <generator class="increment" />
        </id>
        <property name="tpNameStr" type="java.lang.String">
            <column name="tpNameStr" length="10240"/>
        </property>
        <property name="tpMappingMode" type="java.lang.String">
            <column name="TPMAPPINGMODE" />
        </property>
        <property name="ingressTrafficDescriptorNameStr" type="java.lang.String">
            <column name="INGRESSTRAFFICDESCRIPTORNAMESTR" />
        </property>
        <property name="egressTrafficDescriptorNameStr" type="java.lang.String">
            <column name="EGRESSTRAFFICDESCRIPTORNAMESTR" />
        </property>
        <property name="aEndSncNameStr" type="java.lang.String">
        	<column name="aEndSncNameStr"/>
        </property>
        <property name="zEndSncNameStr" type="java.lang.String">
        	<column name="zEndSncNameStr"/>
        </property>
    </class>
</hibernate-mapping>
