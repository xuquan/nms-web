<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.otn.collector.huawei.delivery.beans.subnetworkConnection.CrossConnectAdditionalInfo" table="OTN_DELIVERY_CROSSCONNECTADDITIONALINFO">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="increment" />
        </id>
        <property name="direction" type="java.lang.String">
            <column name="DIRECTION" />
        </property>
        <property name="protectionRole" type="java.lang.String">
            <column name="PROTECTIONROLE" />
        </property>
        <property name="fixed" type="java.lang.String">
            <column name="FIXED" />
        </property>
        <property name="clientType" type="java.lang.String">
            <column name="CLIENTTYPE" />
        </property>
        <property name="clientRate" type="java.lang.String">
            <column name="CLIENTRATE" />
        </property>
        <property name="frequency" type="java.lang.String">
            <column name="FREQUENCY" />
        </property>
        <property name="oduk_TimeSlot" type="java.lang.String">
            <column name="ODUK_TIMESLOT" />
        </property>
        <property name="crossConnectId" type="int">
            <column name="CROSSCONNECTID" />
        </property>
    </class>
</hibernate-mapping>
