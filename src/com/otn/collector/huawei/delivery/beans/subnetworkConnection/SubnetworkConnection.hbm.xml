<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.otn.collector.huawei.delivery.beans.subnetworkConnection.SubnetworkConnection" table="OTN_DELIVERY_SUBNETWORKCONNECTION">
        <id name="nameStr" column="nameStr" type="java.lang.String" length="1024">
			<generator class="assigned"/>
		</id>
        <property name="userLabel" type="java.lang.String">
            <column name="USERLABEL" />
        </property>
        <property name="nativeEMSName" type="java.lang.String">
            <column name="NATIVEEMSNAME" />
        </property>
        <property name="owner" type="java.lang.String">
            <column name="OWNER" />
        </property>
        <property name="sncState" type="java.lang.String">
            <column name="SNCSTATE" />
        </property>
        <property name="direction" type="java.lang.String">
            <column name="DIRECTION" />
        </property>
        <property name="rate" type="short">
            <column name="RATE" />
        </property>
        <property name="staticProtectionLevel" type="java.lang.String">
            <column name="STATICPROTECTIONLEVEL" />
        </property>
        <property name="sncType" type="java.lang.String">
            <column name="SNCTYPE" />
        </property>
        <property name="rerouteAllowed" type="java.lang.String">
            <column name="REROUTEALLOWED" />
        </property>
        <property name="networkRouted" type="java.lang.String">
            <column name="NETWORKROUTED" />
        </property>
        <property name="subnetworkNameStr" type="java.lang.String">
        	<column name="subnetworkNameStr"/>
        </property>
    </class>
</hibernate-mapping>
