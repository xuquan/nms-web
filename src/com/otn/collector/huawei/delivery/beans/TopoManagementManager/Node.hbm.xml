<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.otn.collector.huawei.delivery.beans.TopoManagementManager.Node" table="OTN_DELIVERY_NODE">
        <id name="nameStr" column="nameStr" type="java.lang.String" length="1024">
			<generator class="assigned"/>
		</id>
        <property name="nativeEMSName" type="java.lang.String">
            <column name="NATIVEEMSNAME" />
        </property>
        <property name="nodeType" type="java.lang.String">
            <column name="NODETYPE" />
        </property>
        <property name="parentStr" type="java.lang.String">
            <column name="PARENTSTR" />
        </property>
        <property name="additionalInfoStr" type="java.lang.String">
            <column name="ADDITIONALINFOSTR" length="1024"/>
        </property>
    </class>
</hibernate-mapping>
