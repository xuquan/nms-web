<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.otn.collector.huawei.delivery.beans.TopoManagementManager.Position" table="OTN_DELIVERY_POSITION">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="increment" />
        </id>
        <property name="xPos" type="int" access="field">
            <column name="XPOS" />
        </property>
        <property name="yPos" type="int" access="field">
            <column name="YPOS" />
        </property>
        <property name="nodeNameStr" type="java.lang.String">
        	<column name="nodeNameStr"/>
        </property>
    </class>
</hibernate-mapping>
